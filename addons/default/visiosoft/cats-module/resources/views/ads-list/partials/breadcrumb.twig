{% if params.category %}
    {% for category_breadcrumbs in params.mainCats %}
        <li class="breadcrumb-item">
            <a href="{{ url_route('adv_list_seo', [category_breadcrumbs.slug]) }}"
               class="text-dark font-weight-bold">{{ category_breadcrumbs.name }}
            </a>
        </li>
    {% endfor %}
{% endif %}